@page "/"

<PageTitle>Index</PageTitle>

<p>
	Click the button to be redirected to a Razor page that creates an authentication cookie:
</p>

<p>
	<button @onclick="GoToPage">
		Create identity via Razor Page
	</button>
</p>

@code {

	[Inject]
	NavigationManager navigationManager { get; set; }

	public void GoToPage()
	{
		navigationManager.NavigateTo("/Page01", true);
	}

}